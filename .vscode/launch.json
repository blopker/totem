  "version": "0.2.0",
  "configurations": [
    {
      "name": "totem",
      "request": "launch",
      "type": "dart",
      "args": [
        "--flavor",
        "dev" //dev or prod
    ]
    },
    {
      "name": "totem (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "totem (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    },
    {
      "name": "totem (firebase emulator)",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define=USE_EMULATOR=true"]
    }
  ]
}
