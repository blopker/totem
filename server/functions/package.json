{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "serve": "npm run build -- --watch | firebase emulators:start",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "test": "mocha -r ts-node/register --reporter spec test/**/*.ts",
    "test:emulator": "firebase emulators:exec --only functions,firestore --project totem-dev-184f1  'npm test'",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "16"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@google-cloud/pubsub": "^3.2.0",
    "agora-access-token": "^2.0.4",
    "axios": "1.1.0",
    "date-fns": "^2.29.3",
    "firebase-admin": "^11.0.0",
    "firebase-functions": "^4.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^10.0.0",
    "@types/sinon": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "chai": "^4.3.4",
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.26.0",
    "firebase-functions-test": "^3.0.0",
    "mocha": "^10.0.0",
    "sinon": "^14.0.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "private": true
}
